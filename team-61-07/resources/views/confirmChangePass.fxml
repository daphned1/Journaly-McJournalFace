<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>


<DialogPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <content>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0" prefWidth="292.0">
         <center>
            <Label prefHeight="40.0" prefWidth="247.0" text="Password Reset Successful! Press the OK button to go to Homepage" textAlignment="CENTER" wrapText="true" BorderPane.alignment="CENTER">
               <font>
                  <Font name="Bookman Old Style Italic" size="14.0" />
               </font>
            </Label>
         </center>
         <bottom>
            <Button mnemonicParsing="false" onAction="#okClicked" text="OK" BorderPane.alignment="CENTER" />
         </bottom>
      </BorderPane>
   </content>
</DialogPane>
